<html>
  <head>
    <title>{{ title }}</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="stylesheet" href="/index.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Raleway">
  </head>
  <body>
    {{ comp.templates["web-carousel"]() }}
    <header class="flex justify-between sm:grid sm:grid-cols-1 md:flex md:justify-between justify-items-center bg-gray-200 whitespace-nowrap border-b-2 border-gray-600">
      <nav class="sm:hidden px-3">
        <svg class="h-full w-7" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 14 10">
          <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="
          M1 1 H13 M1 5 H13 M1 9 H13"/>
        </svg>
      </nav>
      <div class="flex flex-row items-center space-x-2 p-1">
        <a href="/" aria-current="page">
          {{ comp.icons.soma({className: "h-full w-8 sm:w-9"}) }}
        </a>
        <div class="text-md whitespace-nowrap overflow-x-auto px-3 sm:hidden">SOMA EASTSIDE CHURCH</div>
        <div class="flex flex-col hidden sm:block">
          <div class="text-xs">SOMA EASTSIDE</div>
          <div class="text-xl font-black scale-y-110 flex justify-center">CHURCH</div>
        </div>
      </div>
      <nav class="hidden sm:block flex-none pt-2 md:pt-0">
        <ul class="flex flex-row h-full">
          <li>
            <details class="relative h-full">
              <summary class="h-full px-4 flex items-center">ABOUT</summary>
              <ul class="absolute w-fit bg-gray-200 z-50 p-5 pt-0 rounded-b-lg border-l-2 border-r-2 border-b-2 border-gray-600 divide-y divide-black">
                <li><a href="/post/mission">OUR MISSION</a>
                <li><details>
                  <summary>OUR TEAM</summary>
                  <ul class="px-3">
                    <li><a href="/post/elders">Elders</a>
                    <li><a href="/post/deacons">Deacons</a>
                    <li><a href="/post/groups">Groups</a>
                  </ul>
                </details>
                <li><a href="/post/first-visit">PLAN YOUR VISIT</a>
                <li><a href="/post/connect">CONNECT WITH US</a>
              </ul>
            </details>
          <li class="px-4 flex items-center">MINISTRIES
          <li class="px-4 flex items-center">EVENTS
          <li class="px-4 flex items-center">GIVE
          <li class="px-4 flex items-center">SERMONS
          <li class="px-4 flex items-center">RESOURCES
        </ul>
      </nav>
    </header>
    <article class="prose p-2 container mx-auto">
    {{ content }}
    </article>
  </body>
</html>
<html>
  <head>
    <title>{{ title }}</title>
    <meta charset="utf-8">
    <link rel="stylesheet" href="/index.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Raleway">
  </head>
  <body>
    {{ comp.templates["web-carousel"]() }}
    <header class="flex flex-row flex-nowrap items-center w-full p-2 space-x-2 bg-gray-200 overflow-hidden h-9">
